{
  "name": "elevenlabs",
  "private": true,
  "scripts": {
    "generate": "turbo run generate --filter=@elevenlabs/types",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "update-ts-references": "^3.6.0",
    "@changesets/cli": "2.27.10",
    "@playwright/test": "^1.44.1",
    "eslint": "8.57.1",
    "@elevenlabs/eslint-config": "workspace:*",
    "husky": "^8.0.0",
    "lint-staged": "15.2.10",
    "next": "15.0.0-canary.23",
    "playwright": "^1.44.1",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "0.2.12",
    "react": "19.0.0-rc-cc1ec60d0d-20240607",
    "react-dom": "19.0.0-rc-cc1ec60d0d-20240607",
    "turbo": "2.4.4",
    "typescript": "5.8.3",
    "vitest": "2.1.4"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": "^18.0.0 || ^20.0.0 || ^22.0.0"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "all",
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
